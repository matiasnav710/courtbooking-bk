{
  "name": "court-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=production && node dist/src/app.js",
    "dev": "export NODE_ENV=development && npx nodemon src/app.ts",
    "watch": "npx tsc --watch",
    "build": "npx tsc",
    "migrate:dev": "export NODE_ENV=development && npx typeorm-ts-node-commonjs migration:run -d ./src/start/connectDatabase.ts",
    "migrate": "export NODE_ENV=production && npx typeorm-ts-node-commonjs migration:run -d ./dist/src/start/connectDatabase.js",
    "revert:dev": "export NODE_ENV=development && npx typeorm-ts-node-commonjs migration:revert -d ./src/start/connectDatabase.ts",
    "revert": "export NODE_ENV=production && npx typeorm-ts-node-commonjs migration:revert -d ./dist/src/start/connectDatabase.js",
    "start:dev": "export NODE_ENV=development && yarn nodemon src/app.ts",
    "generate": "export NODE_ENV=development && npx typeorm-ts-node-commonjs migration:generate ./src/migrations/SchemaUpdate  -d ./src/start/connectDatabase.ts",
    "seed:dev": "export NODE_ENV=development && npx ts-node src/seedes/main.seed.ts",
    "seed": "export NODE_ENV=production && npx ts-node dist/src/seedes/main.seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.9",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "yup": "^1.2.0"
  }
}
